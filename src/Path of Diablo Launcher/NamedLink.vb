Public Class NamedLink

    Private linkName As String
    Private linklink As String

    Property Name As String
        Get
            Return linkName
        End Get
        Set(value As String)
            linkName = value
        End Set
    End Property

    Property Link As String
        Get
            Return linklink
        End Get
        Set(value As String)
            linklink = value
        End Set
    End Property

End Class
